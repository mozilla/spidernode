  _SAVE_LIBS="$LIBS"
            LIBS="$LIBS -latomic"
            cat > conftest.$ac_ext <<EOF
#line 6086 "configure"
#include "confdefs.h"
#include <cstdint>
                 #include <atomic>
int main() {
 std::atomic<uint64_t> foo; foo = 1; 
; return 0; }
EOF
